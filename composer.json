{
  "type": "project",
  "require": {
    "php": "^7.3",
    "ext-bcmath": "^7.3",
    "ext-curl": "^7.3",
    "ext-fileinfo": "^7.3",
    "ext-gd": "^7.3",
    "ext-mbstring": "^7.3",
    "ext-openssl": "^7.3",
    "ext-pdo_mysql": "^7.3",
    "beyondcode/laravel-self-diagnosis": "^1.0",
    "bugsnag/bugsnag-laravel": "^2.0",
    "doctrine/dbal": "^2.2",
    "fideloper/proxy": "^4.0",
    "graham-campbell/markdown": "^13.0",
    "guzzlehttp/guzzle": "~6.2",
    "intervention/image": "~2.4",
    "jenssegers/agent": "~2.5",
    "laravel/framework": "^6",
    "laravel/tinker": "^2.0",
    "spatie/laravel-backup": "^6",
    "superbalist/laravel-google-cloud-storage": "^2.0",
    "thomaswelton/laravel-gravatar": "~1.0",
    "symfony/yaml": "^5.2",
    "webpatser/laravel-uuid": "^3.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "beyondcode/laravel-dump-server": "^1.0",
    "deployer/deployer": "^6.2",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "laravel/browser-kit-testing": "~4.0",
    "mockery/mockery": "^1.0",
    "php-coveralls/php-coveralls": "~2.1",
    "phpunit/phpunit": "^7.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "OpenDominion\\": "src/"
    },
    "classmap": [
      "app/database/factories",
      "app/database/seeds"
    ],
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OpenDominion\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "php artisan package:discover"
    ]
  }
}
